{
  "name": "kubeflow-hugo-theme",
  "version": "1.0.0",
  "description": "A modern, responsive Hugo theme for technical documentation",
  "scripts": {
    "build:css": "tailwindcss -i ./assets/css/main.css -o ./static/css/style.css --watch",
    "build:css:prod": "tailwindcss -i ./assets/css/main.css -o ./static/css/style.css --minify"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "tailwindcss": "^3.4.0"
  }
}